{
	"parser": "Babel-ESLint",
	"env": {
		"browser": true,
		"node": true,
	},
	"plugins": [
		"react",
	],
	"rules": {
		"comma-dangle": [1, "always-multiline"],
		"no-cond-assign": [1, "always"],
		"no-console": 0,
		"no-constant-condition": 0,
		"no-control-regex": 0,
		"no-debugger": 1,
		"no-dupe-args": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty": 1,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 1,
		"no-extra-parens": [1, "functions"],
		"no-extra-semi": 1,
		"no-func-assign": 1,
		"no-inner-declarations": 1,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-negated-in-lhs": 2,
		"no-obj-calls": 2,
		"no-regex-spaces": 2,
		"no-sparse-arrays": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-jsdoc": 2,
		"valid-typeof": 2,
		"no-unexpected-multiline": 1,

		"accessor-pairs": 2,
		"block-scoped-var": 2,
		"complexity": 0,
		"consistent-return": 0,
		"curly": 0,
		"default-case": 2,
		"dot-notation": 0,
		"dot-location": [1, "property"],
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-alert": 2,
		"no-caller": 2,
		"no-div-regex": 1,
		"no-else-return": 0,
		"no-empty-label": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extend-native": 0,
		"no-extra-bind": 1,
		"no-fallthrough": 2,
		"no-floating-decimal": 1,
		"no-implicit-coercion": 2,
		"no-implied-eval": 2,
		"no-invalid-this": 0,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-multi-spaces": [2, { "exceptions": { "JSXOpeningElement": true }}],
		"no-multi-str": 2,
		"no-native-reassign": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-new": 2,
		"no-octal-escape": 2,
		"no-octal": 0,
		"no-param-reassign": 0,
		"no-process-env": 0,
		"no-proto": 0,
		"no-redeclare": [2, {"builtinGlobals": true}],
		"no-return-assign": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-throw-literal": 2,
		"no-unused-expressions": 2,
		"no-useless-call": 1,
		"no-useless-concat": 1,
		"no-void": 2,
		"no-warning-comments": 0,
		"no-with": 2,
		"radix": 0,
		"vars-on-top": 0,
		"wrap-iife": 2,
		"yoda": 2,

		"init-declarations": 0,
		"no-catch-shadow": 0,
		"no-delete-var": 2,
		"no-label-var": 2,
		"no-shadow-restricted-names": 2,
		"no-shadow": 2,
		"no-undef-init": 0,
		"no-undef": 2,
		"no-undefined": 0,
		"no-unused-vars": 1,
		"no-use-before-define": [2, "nofunc"],

		"array-bracket-spacing": [2, "never"],
		"block-spacing": [2, "always"],
		"brace-style": [1, "1tbs"],
		"camelcase": 1,
		"comma-spacing": [1, {"before": false, "after": true}],
		"comma-style": [1, "last"],
		"computed-property-spacing": [1, "never"],
		"consistent-this": [2, "self"],
		"eol-last": 1,
		"func-names": 0,
		"func-style": [2, "declaration"],
		"id-length": 0,
		"id-match": 0,
		"indent": [1, "tab"],
		"jsx-quotes": 1,
		"key-spacing": [1, { "beforeColon": false, "afterColon": true }],
		"lines-around-comment": 0,
		"linebreak-style": [2, "unix"],
		"max-nested-callbacks": [0, 3],
		"new-cap": 0,
		"new-parens": 1,
		"newline-after-var": 0,
		"no-continue": 0,
		"no-inline-comments": 0,
		"no-lonely-if": 2,
		"no-mixed-spaces-and-tabs": [1, "smart-tabs"],
		"no-multiple-empty-lines": [1, { "max": 2 }],
		"no-nested-ternary": 1,
		"no-new-object": 2,
		"no-restricted-syntax": 0,
		"no-spaced-func": 1,
		"no-ternary": 0,
		"no-trailing-spaces": 1,
		"no-underscore-dangle": [1, { "allow": ["__root", "_id"] }],
		"no-unneeded-ternary": 1,
		"object-curly-spacing": 0,
		"one-var": 0,
		"operator-assignment": 0,
		"operator-linebreak": [1, "before"],
		"padded-blocks": [1, "never"],
		"quote-props": [2, "as-needed"],
		"quotes": [1, "single"],
		"require-jsdoc": 0,
		"semi-spacing": [1, {"before": false, "after": true}],
		"semi": [1, "always"],
		"sort-vars": 0,
		"space-after-keywords": [1, "always"],
		"space-before-keywords": [1, "always"],
		"space-before-blocks": [1, "always"],
		"space-before-function-paren": [1, "never"],
		"space-in-parens": [1, "never"],
		"space-infix-ops": 2,
		"space-return-throw-case": 2,
		"space-unary-ops": [1, { "words": true, "nonwords": false }],
		"spaced-comment": 0,
		"wrap-regex": 2,
		"arrow-parens": [1, "as-needed"],
		"arrow-spacing": [1, { "before": true, "after": true }],
		"constructor-super": 2,
		"generator-star-spacing": [1, {"before": false, "after": false}],
		"no-class-assign": 2,
		"no-const-assign": 2,
		"no-dupe-class-members": 2,
		"no-this-before-super": 2,
		"no-var": 0,
		"object-shorthand": 2,
		"prefer-arrow-callback": 2,
		"prefer-const": 2,
		"prefer-spread": 2,
		"prefer-reflect": 2,
		"prefer-template": 2,
		"require-yield": 2,

		"prefer-reflect": 0,
	}
}